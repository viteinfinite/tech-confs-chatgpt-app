  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.3",
    "skybridge": "catalog:"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "nodemon": "^3.1.11",
    "typescript": "^5.7.2"
  }
}
